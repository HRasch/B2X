{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Provider": "PostgreSQL"
  },
  "ConnectionStrings": {
    "SmartDataIntegrationDb": "Host=localhost;Port=5432;Database=B2X_smartdataintegration;Username=postgres;Password=<configure-via-env-or-keyvault>"
  },
  "Messaging": {
    "UseRabbitMq": false
  },
  "RabbitMq": {
    "Uri": "amqp://guest:guest@localhost:5672"
  }
}