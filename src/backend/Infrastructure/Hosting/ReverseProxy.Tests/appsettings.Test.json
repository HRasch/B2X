{
  "ReverseProxy": {
    "Routes": {
      "store-route": {
        "ClusterId": "store-cluster",
        "Match": {
          "Path": "/store/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/store"
          }
        ]
      },
      "admin-route": {
        "ClusterId": "admin-cluster",
        "Match": {
          "Path": "/admin/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/admin"
          }
        ]
      },
      "management-route": {
        "ClusterId": "management-cluster",
        "Match": {
          "Path": "/management/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/management"
          }
        ]
      },
      "api-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "store-cluster": {
        "Destinations": {
          "store-destination": {
            "Address": "http://localhost:8000"
          }
        }
      },
      "admin-cluster": {
        "Destinations": {
          "admin-destination": {
            "Address": "http://localhost:8080"
          }
        }
      },
      "management-cluster": {
        "Destinations": {
          "management-destination": {
            "Address": "http://localhost:8090"
          }
        }
      },
      "api-cluster": {
        "Destinations": {
          "api-destination": {
            "Address": "http://localhost:8100"
          }
        }
      }
    }
  },
  "Multitenancy": {
    "BaseDomain": "b2xgate.com"
  }
}