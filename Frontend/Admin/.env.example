# Admin Frontend Environment Variables

# API Configuration
VITE_ADMIN_API_URL=http://localhost:9000/api/admin
VITE_ADMIN_WS_URL=ws://localhost:9000

# App Configuration
VITE_APP_NAME=B2Connect Admin
VITE_APP_VERSION=1.0.0

# Security: Demo Mode (ONLY for E2E testing - NEVER enable in production!)
# When enabled, allows login with admin@example.com / password
# WARNING: This bypasses authentication - use only for local testing
VITE_ENABLE_DEMO_MODE=false

# Default Tenant ID
VITE_DEFAULT_TENANT_ID=00000000-0000-0000-0000-000000000001

# Optional: Error Tracking
VITE_SENTRY_DSN=

# Optional: Analytics
VITE_GOOGLE_ANALYTICS_ID=
