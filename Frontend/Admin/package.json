{
  "name": "b2connect-admin-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:telemetry": "ENABLE_TELEMETRY=true OTEL_SERVICE_NAME=frontend-admin node --import ./instrumentation.ts node_modules/vite/bin/vite.js",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:auth": "playwright test tests/e2e/auth.spec.ts",
    "e2e:cms": "playwright test tests/e2e/cms.spec.ts",
    "e2e:catalog": "playwright test tests/e2e/shop.spec.ts",
    "e2e:integration": "playwright test tests/e2e/integration.spec.ts",
    "e2e:performance": "playwright test tests/e2e/performance.spec.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "date-fns": "^3.6.0",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.2",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.11.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.57.2",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.57.2",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@playwright/test": "^1.48.2",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.30.0",
    "happy-dom": "^20.0.11",
    "lighthouse": "^13.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "strip-literal": "^3.1.0",
    "tailwindcss": "^4.1.18",
    "terser": "^5.36.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vue-tsc": "^3.2.1"
  }
}
