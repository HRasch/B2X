{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.EntityFrameworkCore": "Debug",
      "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
    }
  },
  "Database": {
    "Provider": "inmemory"
  },
  "Localization": {
    "DefaultLanguage": "en",
    "SupportedLanguages": ["en", "de", "fr", "es", "it", "pt", "nl", "pl"],
    "CacheDuration": 3600,
    "EnableTenantOverrides": true
  },
  "ConnectionStrings": {
    "LocalizationDb": "Host=localhost;Port=5432;Database=b2connect_localization;Username=postgres;Password=postgres;Include Error Detail=true;"
  },
  "AllowedHosts": "*"
}
