      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="coverlet.collector">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Moq" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Testing" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\B2Connect.IdsConnectAdapter\B2Connect.IdsConnectAdapter.csproj" />
    <ProjectReference Include="..\..\..\shared\B2Connect.Shared.Infrastructure\B2Connect.Shared.Infrastructure.csproj" />
  </ItemGroup>

</Project>