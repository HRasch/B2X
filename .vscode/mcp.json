{
  "mcpServers": {
    "b2connect-admin": {
      "command": "node",
      "args": [
        "scripts/mcp-console-logger.js",
        "b2connect-admin",
        "dotnet",
        "run",
        "--project",
        "backend/BoundedContexts/Admin/MCP/B2Connect.Admin.MCP/B2Connect.Admin.MCP.csproj"
      ],
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:8091",
        "DOTNET_DASHBOARD_UNSECURED_ALLOW_ANONYMOUS": "true",
        "LOG_TO_CONSOLE": "true"
      },
      "disabled": false
    },
    "roslyn-code-navigator": {
      "command": "node",
      "args": [
        "scripts/mcp-console-logger.js",
        "roslyn-code-navigator",
        "dotnet",
        "run",
        "--project",
        "tools/RoslynMCP/RoslynMCP.csproj"
      ],
      "env": {
        "DOTNET_ENVIRONMENT": "Production",
        "LOG_LEVEL": "Information",
        "LOG_TO_CONSOLE": "false"
      },
      "disabled": true
    },
    "typescript-mcp": {
      "command": "node",
      "args": [
        "scripts/mcp-console-logger.js",
        "typescript-mcp",
        "node",
        "tools/TypeScriptMCP/dist/index.js"
      ],
      "env": {
        "NODE_ENV": "production",
        "LOG_TO_CONSOLE": "true"
      },
      "disabled": false
