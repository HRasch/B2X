{
	"servers": {
		"com.stackoverflow.mcp/mcp": {
			"type": "http",
			"url": "https://mcp.stackoverflow.com",
			"gallery": "https://api.mcp.github.com",
			"version": "1.1.3"
		},
		"github": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-github"],
			"env": {
				"GITHUB_PERSONAL_ACCESS_TOKEN": "${env:GITHUB_TOKEN}"
			}
		},
		"microsoft-docs": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-microsoft-docs"]
		},
		"filesystem": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-filesystem", "/Users/username/Desktop"]
		},
		"git": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-git"]
		},
		"sqlite": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-sqlite", "--db-path", "/tmp/test.db"]
		},
		"playwright": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-playwright"]
		},
		"chrome-devtools": {
			"command": "npx",
			"args": ["@modelcontextprotocol/server-chrome-devtools"]
		},
		"context7": {
			"command": "npx",
			"args": ["@context7ai/mcp-server"]
		}
	},
	"inputs": []
}